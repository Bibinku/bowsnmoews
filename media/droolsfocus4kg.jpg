<script>

  function getDeliveyInfo(){

    //alert("This is through PDP Page");
	var PDP_selected_sku = $('[name="id"]').val();
	var PDP_selected_qty = parseInt($('[name="quantity"]').val());
    var PDP_request_no = 6987301257278;
	var PDP_zip_code = $('.dtl_input').val(); //110033
    var dtl_per = 'No';
    var dtl_currenttime = new Date();
    dtl_currenttime = dtl_currenttime.getHours();
        
    
    
    	dtl_per = "No";
    
    
    
    
	$('.PDP_dtl_dateOuter').hide();
    $('.PDP_dtl_locOuter').hide();
    $('.PDP_ziperror').show();
	
	var settings = {
		"url": "https://logistic.headsupfortails.com/polls/api/v1/order_delivery_location/",
        //"url": "https://huft-qa.headsupfortails.com/polls/api/v1/order_delivery_location/",
		"method": "POST",
		"timeout": 0,
		"headers": {
			"Content-Type": "application/json"
		},
		"data": JSON.stringify({
			"request_no": PDP_request_no,
			"API_Source": "Product,",
			"pin_code": PDP_zip_code,
			"state": "",
			"api_source": "Product",
			"line_items": [{
				"variant_id": PDP_selected_sku,
				"quantity": PDP_selected_qty,
              "is_personalised": dtl_per
			}]
		}),
	};
        
    console.log(settings);
	
	$.ajax(settings).done(function(response) {
		$('.PDP_ziperror').hide();
		$('.PDP_dtl_response').show();
		console.log(response);
      
		var dtlStatus = response.result.status;
		//var dtlResponseLnt = response.result.response.length; && dtlResponseLnt > 0
      
		 if (dtlStatus == 200) {
		  
		  //var dtl_date = response.result.response[0].edt;
          var dtl_date = response.result.response[0].min_edt; 
          var dtl_max_date = response.result.response[0].max_edt;
          dtl_max_date = new Date(dtl_max_date).toDateString();
           
		  var dtl_pickup = response.result.response[0].self_pickup;
		  //var dtl_Storeloc = response.result.response[0].loc_code;
		  var dtl_Storeloc = response.result.response[0].items[0].loc_code;
           
          var dtl_spilt_sku = response.result.response[0].items.length;
           var dtl_wh_option = response.result.response[0].items[0].within_warehouse_region;

           var DTL_days_range = response.result.response[0].items[0].edt_days;
         
          // date format 
          var dtl_date = new Date(dtl_date).toDateString();
          var dtlTodayDate = new Date().toDateString();
          const dtlTomorrowDate = new Date()
		  dtlTomorrowDate.setDate(new Date().getDate() + 1)
          
          var dtl2days = new Date();
          dtl2days.setDate(new Date().getDate() + 2);
          dtl2days = dtl2days.toDateString().split(' ').slice(0,3).join(' ');
           
           var dtl4days = new Date();
          dtl4days.setDate(new Date().getDate() + 4);
          dtl4days = dtl4days.toDateString().split(' ').slice(0,3).join(' ');
          	
          if(dtl_date === dtlTodayDate){
            var dtl_final_date = "Today"; 
          }else if( dtl_date === dtlTomorrowDate.toDateString()){
            //var dtl_final_date = "Tomorrow";
            var dtl_final_date = "Tomorrow" + ' ('+ dtl_date +' - By 9PM)';
          }else{
            //var dtl_final_date = dtl_date;
            var dtl_final_date = dtl_max_date;
          }
          
            
			

           $('.PDP_dtl_dateOuter').show();

           if (dtl_pickup == "yes") { //new condition 
               if(dtl_spilt_sku > 1){
                 $('.PDP_dtl_date').html("Between "+dtl_final_date+" to "+dtl_max_date);
               }else{
                 $('.PDP_dtl_date').html(dtl_final_date);
    
               }
           }else{ //new condition else 
        
             //$('.PDP_dtl_date').html(DTL_days_range);
             //$('.PDP_dtl_date').html(dtl_max_date);
             $('.PDP_dtl_date').html(dtl_final_date);
      
           }

           
          
           
			//$('.PDP_dtl_date').html(dtl_final_date);
           
          if (dtl_pickup == "yes") {
            $('.PDP_dtl_locOuter').show();
            $('.PDP_dtl_loc').html(dtl_Storeloc);
            
            var str_phone = response.result.response[0].items[0].phone;
            var str_map_link = response.result.response[0].items[0].map_link;
            
            $('.PDP_dtl_phoneOuter').show();
            $('.PDP_dtl_mapOuter').show();

            $('.dtl_str_phone a').html(str_phone);
            $('.dtl_str_phone a').attr('href','tel:'+str_phone+'');

            $('.dtl_str_map a').attr('href',str_map_link);
            
            
          }
		  
		  window.sessionStorage.setItem("DTL_pin", PDP_zip_code);

		}else {
		   $('.PDP_dtl_dateOuter').show();
		   $('.PDP_dtl_date').html("Pincode not serviceable"); 
		}
      
     
		
	});
     
}
  
  $('document').ready(function() {
    var PDP_zip_code = $('.dtl_input').val(); //110033
    
    
    
    var DTL_Pincode = window.sessionStorage.getItem("DTL_pin");
    if(DTL_Pincode){
      
      $('.dtl_input').val(DTL_Pincode)
      getDeliveyInfo();
      $('.dtl_input').hide();
    	$('.dtl_btn').hide();
      	$('.dtl_pin_box div span').html(DTL_Pincode);
    	$('.dtl_pin_box').show();
      
    }
    
    
    $(".dtl_btn").click(function() {
      $('.PDP_dtl_dateOuter').hide();
      $('.PDP_dtl_locOuter').hide();
      $('.PDP_dtl_phoneOuter').hide();
      $('.PDP_dtl_mapOuter').hide();
      
      $('.PDP_ziperror').show();
    	var PDP_zip_code = $('.dtl_input').val(); //110033
        var reg = /^[0-9]+$/;
        if (PDP_zip_code == '') {
            errorMessage = "*Zipcode required!";
          	$('.PDP_ziperror').text(errorMessage);
          	
          
        } else if ((PDP_zip_code.length) < 6 || (PDP_zip_code.length) > 6) {
            errorMessage = "*zipcode should only be 6 digits";
          	$('.PDP_ziperror').text(errorMessage);
          
        } else if (!reg.test(PDP_zip_code)) {
            errorMessage = "*zipcode should be numbers only";
          	$('.PDP_ziperror').text(errorMessage);
          
        } else {
            errorMessage = "Loading...";
          	$('.PDP_ziperror').css('color','#333333');
          	$('.PDP_ziperror').text(errorMessage);
            
          	getDeliveyInfo();
          
      }
      
      

    });
    
    // Cheack other pincode 
    $('.dtl_pin_box button').click(function(){
    	$('.dtl_pin_box').hide();
      	$('.dtl_input').show();
    	$('.dtl_btn').show();
      	$('.dtl_input').val("");
      
    })
    
  
  
  
})

</script>

 
 
            
            
              
                



