    <script>
      $('document').ready(function(){
        $("#whatsapp-message").click(function(){

          if($(this).hasClass('ct_icon')){
            $('body').removeClass('ct_fixed');
            $("#whatsapp-message").removeClass('ct_icon');
        	$('.ct_overlay').hide();
          	$('.ct_popup').hide();
          }else{
          	$('body').addClass('ct_fixed');
          	$(this).addClass('ct_icon');
        	$('.ct_overlay').show();
          	$('.ct_popup').show();
          }
        })

        $(".ct_overlay").click(function(){
          	 $('body').removeClass('ct_fixed');
          	$("#whatsapp-message").removeClass('ct_icon');
        	$('.ct_overlay').hide();
          	$('.ct_popup').hide();
        })
      })
    </script>

