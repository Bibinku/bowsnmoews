        recurpay.product = {
          id: "6987301257278",
          available:"true",
          price: "59640",
          compare_price:"71000",
          variants:[],
          currency:'INR'
        }
        
        recurpay.product.variants.push({
          id: "39790727692350",
          available:"true",
          price: "59640",
          compare_price:"71000",
        })
        
        recurpay.product.variants.push({
          id: "39790727725118",
          available:"true",
          price: "180600",
          compare_price:"215000",
        })
        
        recurpay.product.variants.push({
          id: "39805612752958",
          available:"true",
          price: "495516",
          compare_price:"589900",
        })
        
      
      }
      
      function recurProperties(){
        if($('.recurpay__checkbox').is(":checked")){
          var planName = $('[name="recurpay-input"]:checked').attr('data-plan-name'),
              planId = $('[name="recurpay-input"]:checked').attr('data-plan');

          $('#recurpay_plan_name').attr("name","properties[Subscription]").val(planName);
